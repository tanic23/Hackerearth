#include<iostream>

using namespace std;
int main(){
    int n;
    cin>>n;
    long int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    long int a[n];
    long int k=0;
    for(int i=0;i<n;i++){
         a[i]=arr[i]%10;
    }
   if(a[n-1]==0){
       cout<<("Yes");
   }
   else{
       cout<<("No");
   }
}
